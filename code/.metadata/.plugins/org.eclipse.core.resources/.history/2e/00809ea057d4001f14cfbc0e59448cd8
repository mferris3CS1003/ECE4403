package Assn1.mf;

public class Buyer {
	  private int buyerId;
	  private static int latestBuyerId;
	  private String ACCESS_KEY;
	  private String status;
	public Buyer(String code, String Desc, double cost) {
	    this.buyerId = buyerId;
	    this.status= status;
	    this.latestBuyerId=latestBuyerId;
	    this.ACCESS_KEY=ACCESS_KEY;
	}  
	  public void initializeBuyerId() {
		  buyerId=0;
		  status="MEMBER";
		  
	  }
}
